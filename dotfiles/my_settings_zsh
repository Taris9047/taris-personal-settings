export TERM="xterm-256color"
export HISTCONTROL=ignoredups:erasedups

export LS_MODE='lsd'

function addpath() {
    case ":$PATH:" in
        *":$1:"* ) ;;
        * ) export PATH="$1:$PATH" ;;
    esac
}
